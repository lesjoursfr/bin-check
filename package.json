{
	"name": "@lesjoursfr/bin-check",
	"version": "8.0.4",
	"description": "Check if a binary is working",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/lesjoursfr/bin-check.git"
	},
	"homepage": "https://github.com/lesjoursfr/bin-check#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/bin-check/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": {
		"name": "Kevin MÃ¥rtensson",
		"email": "kevinmartensson@gmail.com",
		"url": "https://github.com/kevva"
	},
	"engines": {
		"node": "22.x || 24.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"binary",
		"check",
		"executable",
		"test"
	],
	"type": "module",
	"dependencies": {
		"execa": "^9.6.1",
		"isexe": "^4.0.0"
	},
	"devDependencies": {
		"@eslint/js": "10.0.1",
		"@tsconfig/node22": "22.0.5",
		"@types/mocha": "10.0.10",
		"@types/node": "24.10.12",
		"eslint": "10.0.0",
		"eslint-config-prettier": "10.1.8",
		"globals": "17.3.0",
		"mocha": "11.7.5",
		"prettier": "3.8.1",
		"ts-node": "10.9.2",
		"typescript": "5.9.3",
		"typescript-eslint": "8.55.0"
	},
	"packageManager": "yarn@4.12.0"
}
