{
	"name": "@lesjoursfr/bin-check",
	"version": "7.1.6",
	"description": "Check if a binary is working",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/lesjoursfr/bin-check.git"
	},
	"homepage": "https://github.com/lesjoursfr/bin-check#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/bin-check/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": {
		"name": "Kevin MÃ¥rtensson",
		"email": "kevinmartensson@gmail.com",
		"url": "https://github.com/kevva"
	},
	"engines": {
		"node": "20.x || 22.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"binary",
		"check",
		"executable",
		"test"
	],
	"type": "module",
	"dependencies": {
		"execa": "^9.5.1",
		"isexe": "^3.1.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.15.0",
		"@tsconfig/node20": "^20.1.4",
		"@types/mocha": "^10.0.9",
		"@types/node": "^22.9.0",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.12.0",
		"mocha": "^10.8.2",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.15.0"
	},
	"packageManager": "yarn@4.5.1"
