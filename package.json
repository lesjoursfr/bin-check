{
	"name": "@lesjoursfr/bin-check",
	"version": "7.1.5",
	"description": "Check if a binary is working",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/lesjoursfr/bin-check.git"
	},
	"homepage": "https://github.com/lesjoursfr/bin-check#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/bin-check/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": {
		"name": "Kevin MÃ¥rtensson",
		"email": "kevinmartensson@gmail.com",
		"url": "https://github.com/kevva"
	},
	"engines": {
		"node": "18.x || 20.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check \"**/*.{ts,js,cjs,mjs,json,yml,md}\"",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write \"**/*.{ts,js,cjs,mjs,json,yml,md}\"",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"binary",
		"check",
		"executable",
		"test"
	],
	"type": "module",
	"dependencies": {
		"execa": "^8.0.1",
		"isexe": "^3.1.1"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.4",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.12.2",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"mocha": "^10.4.0",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.3"
	},
	"packageManager": "yarn@4.1.1"
}
